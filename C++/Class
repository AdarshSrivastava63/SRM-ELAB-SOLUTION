#include <iostream>
#include<math.h>
using namespace std;

class A
{
  public:
  float n;
  public:
  void display()
  {
    cin>>n;
    cout<<n*n<<" ";
  }
};
  class B 
  {
    public:
    float m;
    public :
    void display()
    {
    cin>>m;
    cout<<sqrt(m);
    }
  };
class C : public A, public B
{
public:
};

 int main()
 {
 C sample;
   sample.A::display();
   sample.B::display();
   return 0;
    
 }
    
